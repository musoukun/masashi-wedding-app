{
	"rules": {
		".read": "true",
		".write": "true",
		"media": {
			".indexOn": ["folder", "timestamp", "mediaPath"],
			"$mediaId": {
				".read": true,
				".write": true
			}
		},
		"comments": {
			"$mediaId": {
				".read": true,
				".write": true,
				".indexOn": ["timestamp"]
			}
		}
	}
}
